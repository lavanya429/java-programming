import java.util.*;

public class Main {
    public static void main(String[] args) {
        int num = 28;  
        int i;
        int sum=0;
        
        for(i=1;i<=num/2;i++){
        if (num %i==0){
            sum+=i;
          }
        }
        if (sum > num) {
            System.out.println(num + " is an abundant number.");
        }
        else {
            System.out.println(num + " is not an abundant number.");
        }
        
    }
}
