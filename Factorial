import java.util.*;
  public class App {
     public static void main(String[] args) throws Exception {
         Scanner sc = new Scanner(System.in);
         int n = sc.nextInt();
         int ans = 1;
         if(n <= 1) System.out.println(1);
         else {
             for(int i = n;i>0;i--) ans *= i;
             System.out.println(ans);
         }
         sc.close();
     }
 }
