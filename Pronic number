import java.util.*;

public class Main {
    public static void main(String[] args) {
        int num = 20;  
        int n = 0;

        
        while (n * (n + 1) < num) {
            n++;  
        }

        
        if (n * (n + 1) == num) {
            System.out.println(num + " is a pronic number");
        } 
        else {
            System.out.println(num + " is not a pronic number");
        }
    }
}
